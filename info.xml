  <license>AGPL-3.0</license>
  <maintainer>
    <author>Tobias Lounsbury</author>
    <email>tobiaslounsbury@gmail.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/TobiasLounsbury/civicart</url>
    <url desc="Documentation">https://github.com/TobiasLounsbury/civicart</url>
    <url desc="Support">https://github.com/TobiasLounsbury/civicart/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2017-03-14</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>4.7</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <civix>
    <namespace>CRM/Civicart</namespace>
  </civix>
</extension>
